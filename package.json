{
  "name": "simple-react-hooks",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --ext .ts",
